{
  "name": "karteikarten",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "webci": "export SET NODE_OPTIONS=--openssl-legacy-provider && expo start --web",
    "uiTest": "wait-on http://localhost:19006 && jest",
    "postinstall": "patch-package",
    "test": "concurrently -k \"npm run web\" \"npm run uiTest -- -u --forceExit\" ",
    "testci": "concurrently -k \"npm run webci\" \"npm run uiTest -- -u --forceExit\" "
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom"
  },
